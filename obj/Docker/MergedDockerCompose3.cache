        target: /app
        bind:
          create_host_path: true
      - type: bind
        source: C:\Git\mouts
        target: /src
        bind:
          create_host_path: true
      - type: bind
        source: C:\Users\ferra\vsdbg\vs2017u5
        target: /remote_debugger
        bind:
          create_host_path: true
      - type: bind
        source: C:\Users\ferra\.nuget\packages\
